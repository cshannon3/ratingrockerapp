package com.ratingrocker.sqlitesample;


public class Userdata {

    private int _user_id;
    private String _genrename;
    private String _playlistName;
    private String _playlistid;


    public Userdata(){

    }

    public Userdata(int _user_id, String _genrename, String _playlistName, String _playlistid) {
        this._user_id = _user_id;
        this._genrename = _genrename;
        this._playlistName = _playlistName;
        this._playlistid = _playlistid;
    }

    public int get_user_id() {
        return _user_id;
    }

    public void set_user_id(int _user_id) {
        this._user_id = _user_id;
    }

    public String get_genrename() {
        return _genrename;
    }

    public void set_genrename(String _genrename) {
        this._genrename = _genrename;
    }

    public String get_playlistName() {
        return _playlistName;
    }

    public void set_playlistName(String _playlistName) {
        this._playlistName = _playlistName;
    }

    public String get_playlistid() {
        return _playlistid;
    }

    public void set_playlistid(String _playlistid) {
        this._playlistid = _playlistid;
    }
}
